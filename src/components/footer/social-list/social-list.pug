mixin social-list({ links })
    ul(class = 'social-list')
        each data in links
            li(class = 'social-list__item')
                a(
                    class = 'social-list__link'
                    href = data.href
                    title = data.title
                    aria-label = data.ariaLabel
                )
                    if data.type === 'tw'
